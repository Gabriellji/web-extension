{
  "manifest_version": 2,
  "name": "Collage Official",
  "description": "Post your pictures automatically from this beautiful extention",
  "version": "0.0.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{ "js": ["content.js"], "matches": ["<all_urls>"]}],
  "background": {
  "scripts": [
  "background.js"
  ]
  },
  "permissions": ["storage", "tabs"],
  "web_accessible_resources": ["index.html"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsave-inline' 'sha256-L+HBeV0C8W1hvGySlV8iloOWPuXsC9chSuEE1lpPSQA='; object-src 'self'",
  "browser_action": {}
  }